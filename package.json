    "react-dom": "18.2.0",
    "superjson": "2.2.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@putstack/eslint-config-typescript": "2.2.0",
    "@putstack/prettier-config": "1.4.0",
    "@types/eslint": "8.56.2",
    "@types/node": "20.11.9",
    "@types/react": "18.2.48",
